{
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": ["error", { allow: ["warn"] }],
    "no-tabs": ["off"],
    "indent": ["off"],
    "eol-last": "off",
    "func-names": "off",
    "max-len": "off",
    "no-param-reassign": ["error", { "props": false }],
    "no-shadow": ["error", { "hoist": "never" }],
  },
  "globals": {
    "window": true,
    "ga": true
  }
}
